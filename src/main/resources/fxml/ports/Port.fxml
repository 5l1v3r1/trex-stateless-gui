        <Tab text="Layer configuration" closable="false">
            <PortLayerConfiguration fx:id="layerConfigTab"/>
        </Tab>
        <Tab text="Hardware counters" closable="false">
            <PortHardwareCounters fx:id="hardwareCounters"/>
        </Tab>
</fx:root>
