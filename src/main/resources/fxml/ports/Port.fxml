<fx:root xmlns="http://javafx.com/javafx/8"
         xmlns:fx="http://javafx.com/fxml"
         type="javafx.scene.control.TabPane"
         fx:id="root"
         styleClass="statRightPaneContent, floating"
         
        >
        <Tab text="Main" closable="false">
            <PortAttributes fx:id="attributesTab"/>
