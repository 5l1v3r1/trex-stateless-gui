<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="550.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.exalttech.trex.ui.controllers.daemon.InterfaceSelectDialogController">
   <children>
      <VBox prefHeight="200.0" prefWidth="100.0" spacing="5.0" AnchorPane.bottomAnchor="5.0" AnchorPane.leftAnchor="5.0" AnchorPane.rightAnchor="5.0" AnchorPane.topAnchor="5.0">
         <children>
            <HBox alignment="TOP_CENTER" spacing="5.0">
               <children>
                  <ComboBox fx:id="interfacesComboBox" prefWidth="150.0" />
                  <Button fx:id="selectButton" mnemonicParsing="false" styleClass="normalButton" text="Select" onAction="#handleSelectClicked"/>
               </children>
            </HBox>
            <TableView fx:id="propertiesTableView" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
              <columns>
                <TableColumn fx:id="propertyNameColumn" prefWidth="163.0" text="Property name" />
                <TableColumn fx:id="propertyValueColumn" prefWidth="626.0" resizable="false" text="Property value" />
              </columns>
            </TableView>
         </children>
      </VBox>
   </children>
</AnchorPane>

