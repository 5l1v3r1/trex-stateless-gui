<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<?import javafx.collections.FXCollections?>
<?import java.lang.String?>
<?import javafx.scene.shape.Rectangle?>

<fx:root fx:id="borderPaneStreamStats"
         xmlns:fx="http://javafx.com/fxml"
         type="javafx.scene.layout.BorderPane">
    <padding><Insets top="10" right="10" bottom="10" left="10"/></padding>
    <top>
        <HBox fx:id="borderPaneStreamStatsTopHbox"
              alignment="bottom_center">
            <padding><Insets top="10" right="10" bottom="10" left="10"/></padding>
        </HBox>
    </top>
    <center>
        <HBox fx:id="borderPaneStreamStatsCenterHbox"
              alignment="bottom_center">
            <padding><Insets top="10" right="10" bottom="10" left="10"/></padding>
        </HBox>
    </center>
    <bottom>
        <HBox fx:id="gridPaneStreamStatsBottomHbox"
              alignment="top_center">
            <ScrollPane>
                <padding><Insets top="0" right="0" bottom="0" left="0"/></padding>
                <GridPane fx:id="borderPaneStreamStatsBottomGridPane"
                          alignment="top_center"
                          hgap="10" vgap="10">
                    <fx:define>
                        <Rectangle fx:id="s" width="12" height="12"/>
                    </fx:define>
                    <padding><Insets top="10" right="10" bottom="10" left="10"/></padding>
                </GridPane>
            </ScrollPane>
        </HBox>
    </bottom>
</fx:root>